{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:80"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/dummy-data/{id}",
      "UpstreamHttpMethod": ["Get"],
      "DownstreamPathTemplate": "/Dummy/dummy-string/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [{
        "Host": "api.dummy",
        "Port": 80
      }]
    },
    {
      "UpstreamPathTemplate": "/gateway/data",
      "UpstreamHttpMethod": ["Get"],
      "DownstreamPathTemplate": "/Dummy",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [{
        "Host": "api.dummy",
        "Port": 80
      }]
    }
  ]
}